Prefix sum of 2D array:
#include<bits/stdc++.h>
using namespace std;
int const N=1e3+10;
int arr[N][N];
int pf[N][N];
int main()
{
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cin>>arr[i][j];
            pf[i][j]=arr[i][j]+pf[i][j-1]+pf[i-1][j]-pf[i-1][j-1];
        }
    }
    int t;
    cin>>t;
    while(t--){
        int a,b,c,d;
        cin>>a>>b>>c>>d;
        cout<<pf[c][d]-pf[a-1][b]-pf[c][b-1]+pf[a-1][b-1]<<endl;
    }
}
