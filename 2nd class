#include<bits/stdc++.h>
using namespace std;
const int N=1e5+10;
int arr[N];
int pf[N];
int main()
{
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){  
        //indexing from 1;
        cin>>arr[i];
        pf[i]=pf[i-1]+arr[i];
    }
    int t;
    cin>>t;
    while(t--){
        int l,r;
        cin>>l>>r;
        cout<<pf[r]-pf[l-1]<<endl;
    }
}
